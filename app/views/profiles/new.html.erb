<h1>New Profile Account</h1>

<%= form_for :profiles, url: profiles_path do |f| %>
	<p>
		<%= f.label :username %>
		<%= f.text_field :username %>
	</p>
	<p>
		<%= f.label :picture %>
		<%= f.file_field :picture%>
	</p>
	<p>
		<%= f.submit "Let's go!", :class => 'btn btn-primary' %>
	</p>
<% end %>
